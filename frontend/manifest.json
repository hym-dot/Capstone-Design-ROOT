{
    "manifest_version": 3,
    "name": "Phishing Blocker Test",
    "version": "1.0",
    "description": "안전 테스트용 피싱 차단 시연",
    "permissions": [
        "scripting",
        "tabs",
        "webNavigation"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "warning.css"
            ]
        }
    ]
}